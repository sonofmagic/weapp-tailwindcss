    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^10.4.2",
    "babel-preset-taro": "3.4.1",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.4.1",
    "eslint-plugin-vue": "^6.x",
    "postcss": "^8.4.6",
    "stylelint": "9.3.0",
    "tailwindcss": "^3.0.21",
