:host,
page {
  --spacing: 8rpx;
}
::-webkit-backdrop {
  box-sizing: border-box;
  border: 0 solid;
  margin: 0;
  padding: 0;
}
::backdrop,
:after,
:before {
  box-sizing: border-box;
  border: 0 solid;
  margin: 0;
  padding: 0;
}
.relative {
  position: relative;
}
.flex {
  display: flex;
}
.h-21 {
  height: 168rpx;
  height: calc(var(--spacing) * 21);
}
