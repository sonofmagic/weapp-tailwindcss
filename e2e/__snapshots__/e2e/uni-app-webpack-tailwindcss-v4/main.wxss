}
page:after {
  position: fixed;
  content: '';
  left: -1000px;
  top: -1000px;
  -webkit-animation: shadow-preload 0.1s;
  -webkit-animation-delay: 3s;
  animation: shadow-preload 0.1s;
  animation-delay: 3s;
}
@-webkit-keyframes shadow-preload {
