    "ios",
    "小程序",
    "vite",
    "postcss",
    "webpack",
    "webpack-plugin",
    "gulp",
    "gulp-plugin"
  ],
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/weapp-tailwindcss-webpack-plugin/issues"
  },
  "homepage": "https://weapp-tw.icebreaker.top",
