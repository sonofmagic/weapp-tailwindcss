    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.5",
    "@types/esm": "^3.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
