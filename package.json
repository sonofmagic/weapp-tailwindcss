    "@types/semver": "^7.3.13",
    "@types/vinyl": "^2.0.7",
    "@types/webpack": "^5.28.1",
    "@types/webpack-sources": "^3.2.0",
    "@types/webpack4": "npm:@types/webpack@4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.2.5",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-loader3": "npm:css-loader@3.6.0",
    "defu": "6.1.2",
    "del": "^6.1.1",
    "eslint": "8.39.0",
    "esm": "^3.2.25",
    "execa": "5",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "html-loader": "^4.2.0",
    "html-loader1": "npm:html-loader@1.3.2",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "memfs": "^3.5.1",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "mini-css-extract-plugin1": "npm:mini-css-extract-plugin@1.6.2",
    "miniprogram-automator": "^0.12.0",
    "object-hash": "^3.0.0",
    "open-cli": "^7.2.0",
    "pkg-types": "^1.0.2",
    "postcss-load-config": "^4.0.1",
    "postcss-loader": "^7.3.0",
    "postcss-loader4": "npm:postcss-loader@^4.3.0",
    "postcss-rem-to-responsive-pixel": "^5.1.3",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
