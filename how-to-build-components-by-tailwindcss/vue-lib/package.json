{
  "name": "how-to-build-components-by-tailwindcss-vue",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
