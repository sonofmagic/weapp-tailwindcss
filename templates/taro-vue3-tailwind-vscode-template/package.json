{
  "name": "taro-vue3-tailwind-vscode-template",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": true,
    "css": "Sass",
    "framework": "Vue3"
