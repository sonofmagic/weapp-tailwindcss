{
  "name": "@weapp-tailwindcss/cli",
  "version": "0.0.0-alpha.6",
  "description": "The cli of weapp-tailwindcss",
  "scripts": {
    "dev": "unbuild --sourcemap",
    "build": "unbuild",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "dev:watch": "node test/scripts/watch.js",
    "sync": "cnpm sync @weapp-tailwindcss/cli"
  },
  "keywords": [
    "weapp",
    "tailwindcss",
    "cli",
    "plugin"
  ],
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/weapp-tailwindcss-webpack-plugin/issues"
  },
  "homepage": "https://weapp-tw.icebreaker.top",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "debug": "4.3.4",
    "del": "^7.1.0",
    "fs-extra": "^11.2.0",
    "gulp": "^5.0.0",
    "gulp-if": "^3.0.0",
    "gulp-less": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3"
  },
  "peerDependencies": {
    "less": "*",
    "sass": "*",
    "weapp-tailwindcss": "workspace:*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "less": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}